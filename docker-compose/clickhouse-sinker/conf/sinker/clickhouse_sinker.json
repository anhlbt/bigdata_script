{
    "clickhouse": {
      "cluster": "my",
      "hosts": [
        [
          "clickhouse1"
        ],
        [
          "clickhouse2"
        ]
      ],
      "port": 9000,
      "username": "default",
      "password": "",
      "db": "default",
      "secure": false,
      "insecureSkipVerify": false,
      "retryTimes": 0
    },
  
    "kafka": {
      "brokers": "broker:29092",
      "version": "2.2.1"
    },
  
    "task": {
      "name": "clickhouse_sinker",
      "topic": "event_log",
      "earliest": true,
      "consumerGroup": "sinker",
      "parser": "json",
      "tableName": "event_log_local",
      "dims": [
        {
          "name": "log_id",
          "type": "String",
          "sourceName": "log_id"
        },
        {
          "name": "time",
          "type": "UInt64",
          "sourceName": "time"
        },
        {
          "name": "event",
          "type": "String",
          "sourceName": "event"
        },
        {
          "name": "pid",
          "type": "UInt32",
          "sourceName": "uid"
        }
      ],
      "autoSchema" : false,
      "excludeColumns": [],
      "dynamicSchema": {
        "enable": false,
        "maxDims": 1024
      },
      "shardingKey": "",
      "shardingPolicy": "",
      "flushInterval": 3,
      "bufferSize": 4,
      "timezone": ""
    },
    "logLevel": "info",
    "logPaths": ["stdout", "stderr"]
  }